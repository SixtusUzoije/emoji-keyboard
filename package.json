{
  "name": "emoji-keyboard",
  "version": "1.0.0",
  "description": "Use emojis anywhere on the web!",
  "homepage": "https://justiceo.github.io/emoji-keyboard",
  "scripts": {
    "build": "node tools/esbuild.js",
    "test": "node tools/esbuild.js test",
    "start": "node tools/esbuild.js start",
    "watch": "node tools/esbuild.js watch",
    "start:firefox": "node tools/esbuild.js start --browser=firefox",
    "generateIcons": "node tools/esbuild.js image --icons",
    "translate": "node tools/translateMessages.js",
    "format": "npx prettier . --write",
    "prepare": "husky install"
  },
  "author": "Justice Ogbonna",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/justiceo/emoji-keyboard"
  },
  "keywords": [
    "emoji",
    "keyboard",
    "bitmoji",
    "chrome",
    "firefox",
    "opera",
    "safari",
    "addon",
    "extension",
    "starter",
    "boilerplate"
  ],
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.204",
    "@types/jasmine": "^4.3.1",
    "@types/node": "^18.11.14",
    "@types/puppeteer": "^7.0.4",
    "esbuild": "0.16.4",
    "google-translate-api-x": "^10.5.2",
    "jasmine": "^4.5.0",
    "jimp": "^0.16.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "3.0.3",
    "puppeteer": "^19.4.0"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.2.0",
    "@sentry/browser": "^7.36.0",
    "@webcomponents/custom-elements": "^1.5.1"
  }
}
